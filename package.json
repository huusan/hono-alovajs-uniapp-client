{
  "name": "@huusan/hono-alovajs-uniapp-client",
  "version": "0.1.1",
  "packageManager": "pnpm@10.19.0",
  "description": "A client for Hono that integrates with Alova, specifically adapted for Uniapp environments.",
  "type": "module",
  "license": "MIT",
  "homepage": "https://github.com/huusan/hono-alovajs-uniapp-client#readme",
  "bugs": {
    "url": "https://github.com/huusan/hono-alovajs-uniapp-client/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/huusan/hono-alovajs-uniapp-client.git"
  },
  "author": "huusan <qiyudatui@gmail.com>",
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "eslint --cache .",
    "lint:fix": "pnpm run lint --fix",
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "format": "prettier --cache --write .",
    "release": "bumpp && pnpm publish",
    "all-publish": "pnpm run publish:npm && pnpm publish"
  },
  "peerDependencies": {
    "alova": "3.3.4",
    "hono": "4.10.2"
  },
  "devDependencies": {
    "@hono/node-server": "^1.19.5",
    "@hono/zod-validator": "^0.7.4",
    "@sxzz/eslint-config": "^7.2.7",
    "@sxzz/prettier-config": "^2.2.4",
    "@types/node": "^24.9.1",
    "bumpp": "^10.3.1",
    "eslint": "^9.38.0",
    "prettier": "^3.6.2",
    "tsdown": "^0.15.9",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.2",
    "zod": "^4.1.12"
  },
  "engines": {
    "node": ">=20.18.0"
  },
  "prettier": "@sxzz/prettier-config"
}
